<?xml version="1.0"?>
<configuration>
  <!-- useLegacyV2RuntimeActivationPolicy allows using CLR v2 mixed mode assemblies from CLR v4.
       Therefore it is necessary in order to use System.Data.SQLite.
  -->
  <startup useLegacyV2RuntimeActivationPolicy="true">
     <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
</configuration>
